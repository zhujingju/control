<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable  name="butt"
    >
    <attr name="myString" format="string"></attr>
    <attr name="myColor" format="color"></attr>
</declare-styleable>

<declare-styleable  name="roundedimageview"
    >
    <attr name="border_outside_color" format="color"></attr>
    <attr name="border_thickness" format="dimension"></attr>
    <attr name="border_inside_color" format="color"></attr>
</declare-styleable>

<declare-styleable name="Dialog">
        <attr name="DialogTitleAppearance" format="reference" />
        <attr name="DialogTitleText" format="reference|string" />
        <attr name="DialogSpotColor" format="reference|color"/>
        <attr name="DialogSpotCount" format="integer"/>
    </declare-styleable>


 <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    
 
 <attr name="rightPadding" format="dimension"></attr>

    <declare-styleable name="SlidingMenu">
        <attr name="rightPadding"></attr>
    </declare-styleable>

    <declare-styleable name="slideswitch">
        <attr name="themeColor" format="color" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="2" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="NiceSpinner">
        <attr name="arrowTint" format="color" />
        <attr name="hideArrow" format="boolean" />
        <attr name="backgroundSelector" format="integer"/>
        <attr name="textTint" format="color"/>
    </declare-styleable>
</resources>
